body {
	background: #539598;
	margin: 0;

.header-main {
	background: #DE5246;
}

.wrapper {
	width: 1170px;
	margin: 0 auto;
}

.header {
	overflow: hidden;
}

.logo {
	width: 20%;
	float: left;
}

.nav {
	width: 80%;
	float: right;
}

.nav ul {
	list-style: none;
	float: right;
}

.nav ul li {
	display: inline-block;
}

.nav ul li a {
	text-decoration: none;
	font-size: 20px;
	color: #fff;
	padding: 10px 15px;
	border-radius: 10px;
}

.nav ul li a:hover {
	background: #fff;
	color: #DE5246;
}